<?xml version="1.0" encoding="GBK"?>

<!-- 模板：flash动画与AS3交互的XML文档模板 -->
<!--equipstate节点名不能修改，每一类型的设备，对应一个equip节点，eqTypeId为设备类型ID-->
<!--1、element节点为设备Id节点，模板值为空-->
<!--2、AxisX节点为小方块展示X坐标，模板值不为空-->
<!--3、AxisY节点为小方块展示Y坐标，模板值不为空-->
<!--4、State节点为设备状态值，模板值不能为空，默认值为3（未监控）-->
<!--5、run属性代表的是专家论证运行状态编号，warn属性填写专家论证故障状态编号  -->
<equipstate> 
  <!--系统季节标识-->  
  <sysTm> 
    <State>1</State> 
  </sysTm>  
  
   <!--冷却塔1-->  
  <Equip eqTypeId="1004001" run="03" warn="04" keyName="ta1"> 
    <element>coolTower1</element>  
    <AxisX>-10</AxisX>  
    <AxisY>-10</AxisY>  
    <AxisA>0.6</AxisA>  
    <AxisB>-71.15</AxisB>  
    <State>3</State> 
  </Equip>  
  <!--冷却塔2-->  
  <Equip eqTypeId="1004001" run="03" warn="04" keyName="ta2"> 
    <element>coolTower2</element>  
    <AxisX>-10</AxisX>  
    <AxisY>-10</AxisY>  
    <AxisA>0.75</AxisA>  
    <AxisB>-72.90</AxisB>  
    <State>3</State> 
  </Equip>  
    <!--冷却塔3-->  
  <Equip eqTypeId="1004001" run="03" warn="04" keyName="ta3"> 
    <element>coolTower3</element>  
    <AxisX>-10</AxisX>  
    <AxisY>-10</AxisY>  
    <AxisA>0.75</AxisA>  
    <AxisB>-72.90</AxisB>  
    <State>3</State> 
  </Equip>
  <!--冷却塔4-->  
  <Equip eqTypeId="1004001" run="03" warn="04" keyName="ta4"> 
    <element>coolTower3</element>  
    <AxisX>-10</AxisX>  
    <AxisY>-10</AxisY>  
    <AxisA>0.75</AxisA>  
    <AxisB>-72.90</AxisB>  
    <State>3</State> 
  </Equip>   
</equipstate>
