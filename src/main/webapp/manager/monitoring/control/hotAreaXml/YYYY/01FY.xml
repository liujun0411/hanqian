	<area title="负压泵2#" id="xyb" wd="18" hg="18" marLeft="336" marTop="425"  prop="xiYinBeng" initFlag="hide" eqTypeId="11002" positionX="" 	postionY="" eventType="click"></area>
	<area title="负压泵3#" id="xyb" wd="18" hg="18" marLeft="400" marTop="390"  prop="xiYinBeng" initFlag="hide" eqTypeId="11002" positionX=""  postionY="" eventType="click"></area>
</equipment>