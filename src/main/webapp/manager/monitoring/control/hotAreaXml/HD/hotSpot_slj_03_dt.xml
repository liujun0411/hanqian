	<!-- 集水器 -->
	<area title="集水器" wd="0" hg="0" marLeft="-1000" marTop="-1020"
		prop="coolTower" initFlag="hide" eqTypeId="1004006" positionX=""
		postionY="" eventType="click"></area>

</equipment>