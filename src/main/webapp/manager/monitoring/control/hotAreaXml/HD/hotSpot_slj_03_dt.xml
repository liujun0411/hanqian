<?xml version="1.0" encoding="utf-8"?>
	<!-- 热区绘制说明 -->
	<!--
		1、equipment节点名不能改变，里面的子项全部是area，id=”自定义名字“，
		wd是热区宽度，hg是热区高度，marLeft是热区左边距，marTop是热区上边距，prop是专家论证监测点节点，initFlag是否初始化页面展示（hide或show），
		eqTypeId是指设备类型ID,positionX是默认的X轴坐标，postionY是默认y轴坐标
	-->
<equipment>
	<!-- 热水泵1 -->
	<area title="热水泵1#" wd="30" hg="25" marLeft="225" marTop="480"
		prop="hotBeng" initFlag="hide" eqTypeId="1004007" positionX=""
		postionY="" eventType="click"></area>
		
	<!-- 热水泵2-->
	<area title="热水泵2#" wd="30" hg="25" marLeft="200" marTop="460"
		prop="hotBeng" initFlag="hide" eqTypeId="1004007" positionX=""
		postionY="" eventType="click"></area>
		
	<!-- 热水二次泵1 -->
	<area title="热水补水泵1#" wd="30" hg="25" marLeft="390" marTop="530"
		prop="hotBeng" initFlag="hide" eqTypeId="1004007" positionX=""
		postionY="" eventType="click"></area>
		
	<!-- 热水二次泵2 -->
	<area title="热水补水泵2#" wd="30" hg="25" marLeft="435" marTop="510"
		prop="hotBeng" initFlag="hide" eqTypeId="1004007" positionX=""
		postionY="" eventType="click"></area>

	<!-- 换热器1 -->
	<area title="换热器1#" wd="30" hg="25" marLeft="360" marTop="675"
		prop="hrq" initFlag="hide" eqTypeId="1004008" positionX="" postionY=""
		eventType="click"></area>
		
	<!-- 换热器2-->
	<area title="换热器2#" wd="30" hg="25" marLeft="270" marTop="630"
		prop="hrq" initFlag="hide" eqTypeId="1004008" positionX="" postionY=""
		eventType="click"></area>

	<!-- 分水器 -->
	<area title="分水器" wd="0" hg="0" marLeft="-1000" marTop="-1020"
