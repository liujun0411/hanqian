<?xml version="1.0" encoding="utf-8"?>
<!-- 热区绘制说明 -->
<!-- 1、equipment节点名不能改变，里面的子项全部是area，id=”自定义名字“，
wd是热区宽度，hg是热区高度，marLeft是热区左边距，marTop是热区上边距，prop是专家论证监测点节点，initFlag是否初始化页面展示（hide或show），
eqTypeId是指设备类型ID,positionX是默认的X轴坐标，postionY是默认y轴坐标 -->
<equipment>
	<child>
	
	   	<!-- 冷冻泵1 -->
	   <area title="冷冻泵1#" wd="30" hg="25" marLeft="500" marTop="605" prop="coolDong" initFlag="hide" eqTypeId="1004003" positionX="" postionY="" eventType="click"></area>
	   	<!-- 冷冻泵2 -->
	   <area title="冷冻泵2#" wd="30" hg="25" marLeft="590" marTop="570" prop="coolDong" initFlag="hide" eqTypeId="1004003" positionX="" postionY="" eventType="click"></area>
	   	<!-- 冷冻泵3 -->
	   <area title="冷冻泵3#" wd="30" hg="25" marLeft="655" marTop="540" prop="coolDong" initFlag="hide" eqTypeId="1004003" positionX="" postionY="" eventType="click"></area>
	   	
	   	<!-- 换热器1 -->
	   <area title="换热器1#" wd="30" hg="25" marLeft="210" marTop="500" prop="hrq" initFlag="hide" eqTypeId="1004008" positionX="" postionY="" eventType="click"></area>
	   	<!-- 换热器2-->
	   <area title="换热器2#" wd="30" hg="25" marLeft="290" marTop="490" prop="hrq" initFlag="hide" eqTypeId="1004008" positionX="" postionY="" eventType="click"></area>
		<!-- 换热器3-->
	   <area title="换热器3#" wd="30" hg="25" marLeft="380" marTop="460" prop="hrq" initFlag="hide" eqTypeId="1004008" positionX="" postionY="" eventType="click"></area>
		
		<!-- 分水器 -->
	   <area title="分水器" wd="0" hg="0" marLeft="-1000" marTop="-1020" prop="coolTower" initFlag="hide" eqTypeId="1004005" positionX="" postionY="" eventType="click"></area>
	    <!-- 集水器 -->
	   <area title="集水器" wd="0" hg="0" marLeft="-1000" marTop="-1020" prop="coolTower" initFlag="hide" eqTypeId="1004006" positionX="" postionY="" eventType="click"></area>
		
	</child>
</equipment>