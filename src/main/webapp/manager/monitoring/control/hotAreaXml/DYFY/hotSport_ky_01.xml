<?xml version="1.0" encoding="utf-8"?>
	<!-- 热区绘制说明 -->
	<!--
		1、equipment节点名不能改变，里面的子项全部是area，id=”自定义名字“，
		wd是热区宽度，hg是热区高度，marLeft是热区左边距，marTop是热区上边距，prop是专家论证监测点节点，initFlag是否初始化页面展示（hide或show），
		eqTypeId是指设备类型ID
	-->
<equipment>
	<!-- 压力罐1 -->
	<area title="压力罐1#" id="fyg" wd="18" hg="18" marLeft="799" marTop="442"
		prop="newFau" initFlag="hide" eqTypeId="8002" positionX=""
		postionY="" eventType="click"></area>
    <!-- 压力罐2 -->
	<area title="压力罐2#" id="fyg" wd="18" hg="18" marLeft="643" marTop="394"
		prop="newFau" initFlag="hide" eqTypeId="8002" positionX=""
		postionY="" eventType="click"></area>	
	<!-- 空压机1 -->
