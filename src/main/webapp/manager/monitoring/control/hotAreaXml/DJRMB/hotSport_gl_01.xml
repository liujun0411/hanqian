			prop="feedTank" initFlag="hide" eqTypeId="2002" positionX=""
			postionY="" eventType="click"></area>

		<!-- 给水泵1 -->
		<area title="给水泵1#" id="gxb" wd="18" hg="18" marLeft="240"
			marTop="470" prop="feedPump" initFlag="hide" eqTypeId="2003"
			positionX="" postionY="" eventType="click"></area>

		<!-- 给水泵2 -->
