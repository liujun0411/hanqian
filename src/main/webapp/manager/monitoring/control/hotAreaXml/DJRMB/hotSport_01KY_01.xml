<?xml version="1.0" encoding="utf-8"?>
<!-- 热区绘制说明 -->
<!-- 1、equipment节点名不能改变，里面的子项全部是area，id=”自定义名字“，
wd是热区宽度，hg是热区高度，marLeft是热区左边距，marTop是热区上边距，prop是专家论证监测点节点，initFlag是否初始化页面展示（hide或show），
eqTypeId是指设备类型ID -->
<equipment>
<child>
   <!-- 空压机1-->
   <area title="空压机1#" id="kyj1" wd="30" hg="20" marLeft="215" marTop="493" prop="KongYaJi" initFlag="hide" eqTypeId="8001" positionX="200" postionY="500" eventType="click"></area>
   <!-- 空压机2-->
   <area title="空压机2#" id="kyj2" wd="30" hg="20" marLeft="334" marTop="449" prop="KongYaJi" initFlag="hide" eqTypeId="8001" positionX="" postionY="" eventType="click"></area>
   <!-- 空压机3-->
   <area title="空压机3#" id="kyj3" wd="30" hg="20" marLeft="430" marTop="413" prop="KongYaJi" initFlag="hide" eqTypeId="8001" positionX="" postionY="" eventType="click"></area>
   <!--压力罐1#-->
   <area title="压力罐" wd="30" hg="30" marLeft="640" marTop="310" prop="YaLiGuan" initFlag="hide" eqTypeId="8002" positionX="" postionY="" eventType="click"></area>
</child>
</equipment>