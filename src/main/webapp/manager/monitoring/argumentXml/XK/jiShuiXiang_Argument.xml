<?xml version="1.0" encoding="utf-8"?>
<!--设备信息-->
<!-- run代表检测点位编号，des代表点位描述，unit是监测点单位，maxVa是监测点最大取值，minVa是监测点最小取值，remark是点位读数对应备注信息，condition是点位的前置条件，seTitle是附表题，showFlag是否展示（1代表展示，0代表不展示） -->
<equipment title="冷却塔" hasParTitle="true">

   <child title="冷却塔">
        <property run="01" des="温度" unit="℃" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
       <property run="02" des="累计流量" unit="T" colu="add(01,02,03)" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
       <property run="03" des="液位" unit="mm" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>  
       <property run="04" des="液位低报警" unit="" colu="" maxVa="" minVa="" remark="0|正常,1|报警" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
   </child>
   
</equipment>