<?xml version="1.0" encoding="utf-8"?>
	<!-- 热区绘制说明 -->
	<!--
		1、equipment节点名不能改变，里面的子项全部是area，id=”自定义名字“，
		wd是热区宽度，hg是热区高度，marLeft是热区左边距，marTop是热区上边距，prop是专家论证监测点节点，initFlag是否初始化页面展示（hide或show），
		eqTypeId是指设备类型ID,positionX是默认的X轴坐标，postionY是默认y轴坐标
	-->
<equipment>
	<child>
		<!-- 分水器 -->
		<area title="分水器" eventType="click" wd="0" hg="0" marLeft="-1000"
			marTop="-1020" prop="coolTower" initFlag="hide" eqTypeId="1004005"
			positionX="" postionY=""></area>
			
		<!-- 集水器 -->
		<area title="集水器" eventType="click" postionY="" positionX=""
			eqTypeId="1004006" initFlag="hide" prop="others" marTop="-1020"
			marLeft="-1000" hg="0" wd="0" />
		
		<!-- 换热器1 -->
		<area title="换热器1#" eventType="click" wd="30" hg="25" marLeft="360"
			marTop="440" prop="hrq" initFlag="hide" eqTypeId="1004008" positionX="95"
			postionY="390"></area>
			
		<!-- 换热器2-->
		<area title="换热器2#" eventType="click" wd="30" hg="25" marLeft="320"
			marTop="425" prop="hrq" initFlag="hide" eqTypeId="1004008" positionX="330"
			postionY="390"></area>
		
		<!-- 换热器3-->
		<area title="换热器3#" eventType="click" wd="30" hg="25" marLeft="275"
			marTop="415" prop="hrq" initFlag="hide" eqTypeId="1004008" positionX="330"
			postionY="390"></area>
		<!-- 热水总管-->
		<area title="热水总管" eventType="click" wd="30" hg="25" marLeft="275"
			marTop="415" prop="hrq" initFlag="hide" eqTypeId="1004014" positionX="340"
			postionY="400"></area>
	</child>
</equipment>