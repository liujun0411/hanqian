<?xml version="1.0" encoding="utf-8"?>
<!--设备信息-->
<!-- run代表检测点位编号，des代表点位描述，unit是监测点单位，maxVa是监测点最大取值，minVa是监测点最小取值，remark是点位读数对应备注信息，condition是点位的前置条件，seTitle是附表题，showFlag是否展示（1代表展示，0代表不展示）,isAbs是否取绝对值(1代表取，0代表不取) -->
<equipment title="新风机">
   <FENGJI title="风机">
	   <property run="03" des="故障状态" unit="" colu="" maxVa="" minVa="" remark="0|正常,1|故障" condition="" seTitle="" showFlag="0" isAbs="1" errText="故障"></property>
	   <property run="04" des="运行状态" unit="" colu="" maxVa="" minVa="" remark="0|停止,1|运行" condition="03" seTitle="运行状态" showFlag="1" isAbs="1" errText=""></property>
	   <property run="05" des="操作方式" unit="" colu="" maxVa="" minVa="" remark="0|手动,1|自动" condition="" seTitle="" showFlag="1" isAbs="1" errText=""></property>
   </FENGJI>
   <newFeng title="新风管">
       <property run="10" des="防冻温度" unit="℃" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
	 </newFeng>
   <outfeng title="出风管">
       <property run="07" des="机外余压" unit="pa" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
       <property run="08" des="送风温度" unit="℃" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
       <property run="21" des="送风湿度" unit="%" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
   </outfeng>
   <fauLvW title="滤网">
       <property run="12" des="滤网压差" unit="" colu="" maxVa="" minVa="" remark="0|正常,1|需清洗" condition="" seTitle="" showFlag="1" isAbs="1" errText="需清洗"></property>
   </fauLvW>
</equipment>