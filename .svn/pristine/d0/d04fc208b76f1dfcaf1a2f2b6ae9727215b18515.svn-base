<equipment title="冷却塔" hasParTitle="true">
   <child title="冷却塔">
	<property run="01" des="进水温度" unit="℃" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
	<property run="02" des="旁通阀开度" unit="%" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
	<property run="03" des="运行状态" unit="" colu="" maxVa="" minVa="" remark="0|停止,1|运行" condition="04" seTitle="运行状态" showFlag="1" isAbs="1" errText=""></property>
	<property run="04" des="故障状态" unit="" colu="" maxVa="" minVa="" remark="0|正常,1|故障" condition="" seTitle="" showFlag="0" isAbs="1" errText="故障"></property>
	<property run="05" des="变频反馈" unit="Hz" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
	<property run="06" des="出水温度" unit="℃" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
	<property run="07" des="操作方式" unit="" colu="" maxVa="" minVa="" remark="0|手动,1|自动" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
   </child>
   <child title="冷却进水管道">
	<property run="08" des="进水温度" unit="℃" colu="0" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
	<property run="09" des="进水压力" unit="Mpa"  colu="0" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
	<property run="10" des="累计流量" unit="T" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
	<property run="11" des="瞬时流量" unit="T/h" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
   </child>
   <child title="冷却出水管道">
	<property run="12" des="出水温度" unit="℃"  colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
	<property run="13" des="出水压力" unit="Mpa"  colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
	<property run="14" des="累计流量" unit="T" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
	<property run="15" des="瞬时流量" unit="T/h" colu="" maxVa="" minVa="" remark="" condition="" seTitle="" showFlag="1" isAbs="0" errText=""></property>
   </child>
</equipment>