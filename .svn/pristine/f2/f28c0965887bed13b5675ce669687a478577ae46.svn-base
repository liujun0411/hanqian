package com.hanqian.util;



/**
 * 区域名
 * @author clczp
 *
 */
public class SysQuYuName {
	private SysQuYuName(){}
	
	public static String getName(String quyuId){
		String name="";
		if ("1".equals(quyuId)) {
			name="门诊";
		}else if ("2".equals(quyuId)) {
			name="急诊";
		}else if ("3".equals(quyuId)) {
			name="住院";
		}else if ("4".equals(quyuId)) {
			name="医技";
		}else if ("5".equals(quyuId)) {
			name="保障";
		}else if ("6".equals(quyuId)) {
			name="行政";
		}else if ("7".equals(quyuId)) {
			name="科研";
		}else if ("8".equals(quyuId)) {
			name="教育";
		}else if ("9".equals(quyuId)) {
			name="生活";
		}else if ("10".equals(quyuId)) {
			name="车库";
		}else if ("11".equals(quyuId)) {
			name="其它";
		}
		
		return name;
	}
}
