<?xml version="1.0" encoding="GBK"?>

<!-- 模板：flash动画与AS3交互的XML文档模板 -->
<!--equipstate节点名不能修改，每一类型的设备，对应一个equip节点，eqTypeId为设备类型ID-->
<!--1、element节点为设备Id节点，模板值为空-->
<!--2、AxisX节点为小方块展示X坐标，模板值不为空-->
<!--3、AxisY节点为小方块展示Y坐标，模板值不为空-->
<!--4、State节点为设备状态值，模板值为空-->
<equipstate> 
	
  <!--给水泵 1-->  
  <Equip eqTypeId="2003" run="02" warn="01" keyName="beng1">  
    <element>coolTower1</element>  
    <AxisX>-10</AxisX>  
    <AxisY>-10</AxisY>  
    <AxisA>0</AxisA>  
    <AxisB>0</AxisB> 
    <State>2</State> 
  </Equip>
  <!--给水泵 2-->  
  <Equip eqTypeId="2003" run="02" warn="01" keyName="beng2">  
    <element>coolTower1</element>  
    <AxisX>-10</AxisX>  
    <AxisY>-10</AxisY>  
    <AxisA>0</AxisA>  
    <AxisB>0</AxisB> 
    <State>2</State> 
  </Equip>
  <!--给水泵 3-->  
  <Equip eqTypeId="2003" run="02" warn="01" keyName="beng3">  
    <element>coolTower1</element>  
    <AxisX>-10</AxisX>  
    <AxisY>-10</AxisY>  
    <AxisA>0</AxisA>  
    <AxisB>0</AxisB> 
    <State>2</State> 
  </Equip>
  <!--给水泵 4-->  
  <Equip eqTypeId="2003" run="02" warn="01" keyName="beng4">  
    <element>coolTower1</element>  
    <AxisX>-10</AxisX>  
    <AxisY>-10</AxisY>  
    <AxisA>0</AxisA>  
    <AxisB>0</AxisB> 
    <State>2</State> 
  </Equip>

  <!--锅炉本体 1-->  
  <Equip eqTypeId="2001" run="10" warn="11" keyName="guolu1"> 
    <element>coolTower1</element>  
    <AxisX>-10</AxisX>  
    <AxisY>-10</AxisY>  
    <AxisA>0</AxisA>  
    <AxisB>0</AxisB> 
    <State>2</State> 
  </Equip>  
