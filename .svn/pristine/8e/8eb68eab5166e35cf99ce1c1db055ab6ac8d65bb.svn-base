		prop="coolQue" initFlag="hide" eqTypeId="1004002" positionX=""
		postionY="" eventType="click"></area>
	<!-- 冷却泵2 -->
	<area title="冷却泵2#" wd="15" hg="15" marLeft="305" marTop="403"
		prop="coolQue" initFlag="hide" eqTypeId="1004002" positionX="150"
		postionY="355" eventType="click"></area>


	<!-- 冷却塔B1# -->
