<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!-- 模板：flash动画与AS3交互的XML文档模板 -->
<!--equipstate节点名不能修改，每一类型的设备，对应一个equip节点，eqTypeId为设备类型ID-->
<!--1、element节点为设备Id节点，模板值为空-->
<!--2、AxisX节点为小方块展示X坐标，模板值不为空-->
<!--3、AxisY节点为小方块展示Y坐标，模板值不为空-->
<!--4、State节点为设备状态值，模板值为空-->
<equipstate>
   <!--空压机1-->
	<Equip eqTypeId="8001" run="01" warn="02">
		<element>coolTower1</element>
		<AxisX>-10</AxisX>
		<AxisY>-10</AxisY>
		<AxisA>-0.15</AxisA>
		<AxisB>-139.9</AxisB>
		<State>1</State>
	</Equip>
	 <!--空压机2-->
	<Equip eqTypeId="8001" run="01" warn="02">
		<element>coolTower2</element>
		<AxisX>-10</AxisX>
		<AxisY>-10</AxisY>
		<AxisA>-0.1</AxisA>
		<AxisB>-140.1</AxisB>
		<State>1</State>
	</Equip>
	 <!--空压机3-->
	<Equip eqTypeId="8001" run="01" warn="02">
		<element>coolTower3</element>
		<AxisX>-10</AxisX>
		<AxisY>-10</AxisY>
		<AxisA>-0.15</AxisA>
		<AxisB>-139.85</AxisB>
		<State>1</State>
	</Equip>
	<!--压力罐-->
	<Equip eqTypeId="8002">
		<element>coolTower4</element>
		<AxisX>-10</AxisX>
		<AxisY>-10</AxisY>
		<AxisA>-0.15</AxisA>
		<AxisB>-139.85</AxisB>
		<State>1</State>
	</Equip>
</equipstate>