@charset "utf-8";
/* CSS Document */
body{
		  margin-left: 0px;
		  margin-top: 0px;
		  margin-right: 0px;
		  margin-bottom: 0px;
		  margin:0 auto;
		  padding:0;
		  overflow:auto;
		  background:url(../images/common/index_title_bg3.jpg) no-repeat;
      }
      #body_warpper{
        height: 300px;
      }
      .index_title{
		 margin-left:62px;
		 padding-top:25px;
		 padding-bottom:25px;
	  }
	  .index_title .hosp_logo{
		  height:60px;
		  height:60px;
	  }
	  .index_title .td_logo{
		  padding-right:20px;
		  width:90px;
	  }
	  .index_title .td_title{
		   padding-left:15px;
		   width:667px;
	  }
	  .index_title .top_desc{
		  color:white;
		  font-size:14px;
		  font-family:'黑体';
		  margin-right:40px;
	  }
	  .index_title .aHref{
		  color:white;
		  font-size:14px;
		  font-family:'黑体';
		  margin-right:20px;
		  text-decoration:none;
	  }
		  
	  .index_title a.aHref:hover{
		  color:white;
		  font-size:14px;
		  font-family:'黑体';
		  margin-right:20px;
		  text-decoration:underline;
	  }
	  .top_menus{
		  border:0px solid green;
		  height:42px;
		  
	  }
	  .top_menus .toptd_1366{
		  width:48px;
		  height:42px;
		  margin-left:-3px;
		  margin-top:-1px;
		  background:url(../images/common/lvse.png) no-repeat;
	  }
	  
	  .top_menus .toptd_1440{
		  width:71px;
		  height:42px;
		  margin-left:-3px;
		  margin-top:-1px;
		  background:url(../images/common/lvse.png) no-repeat;
	  }
	  .top_menus .menus_td{
		  background:url(../images/common/top_menu_tdbg1.png) repeat-x;
		  width:120px;
		  border-left:2px solid #6F7072;
		  height:42px;
	  }
	  .top_menus .menus_td a{
		  font-size:15px;
		  font-family:'黑体';
		  color:#404041;
		  font-weight:bold;
		  text-decoration:none;
		  cursor: pointer;
	  }
	  
	  .top_menus .menus_td .ahover{
		  font-size:15px;
		  font-family:'黑体';
		  color:#FE8B03;
		  font-weight:bold;
		  text-decoration:none;
		  cursor: pointer;
	  }
	  
	  .top_menus .menus_td .selected{
		  font-size:15px;
		  font-family:'黑体';
		  color:#0057A6;
		  font-weight:bold;
		  text-decoration:none;
		  cursor: pointer;
	  }
	  .top_menus .last_td{
		  background:url(../images/common/top_menu_tdbg1.png) repeat-x;
		  border-left:2px solid #6F7072;
		  padding-right:40px;
	  }
	  .top_menus .last_td span{
		  font-size:15px;
		  font-family:'黑体';
		  color:#404041;
		  font-weight:bold;
		  line-height:42px;
	  }
	  .top_menus .last_td img{
	      margin-bottom:-5px;
	      margin-right:10px;
	  }
