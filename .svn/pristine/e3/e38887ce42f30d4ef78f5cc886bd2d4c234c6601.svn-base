public class VOShowPoint {
	
	private String equipId;
	
	private String pointName;
	
	private String projectPoint;
	
	private String record;
	private String controlPoint;
	
	/**
	 * @param equipId
	 * @param pointName
	 * @param projectPoint
	 * @param record
	 */
	public VOShowPoint(String equipId, String pointName, String projectPoint,String controlPoint,
			String record) {
		super();
		this.equipId = equipId;
		this.pointName = pointName;
		this.projectPoint = projectPoint;
		this.controlPoint = controlPoint;
		this.record = record;
	}
	public VOShowPoint(String equipId, String pointName, String projectPoint,
			String record) {
		super();
		this.equipId = equipId;
		this.pointName = pointName;
		this.projectPoint = projectPoint;
		this.record = record;
	}
	public String getEquipId() {
		return equipId;
	}

	public void setEquipId(String equipId) {
		this.equipId = equipId;
	}

	public String getPointName() {
		return pointName;
	}

	public void setPointName(String pointName) {
		this.pointName = pointName;
	}

	public String getProjectPoint() {
		return projectPoint;
	}

	public void setProjectPoint(String projectPoint) {
		this.projectPoint = projectPoint;
	}

	public String getRecord() {
		return record;
	}

	public void setRecord(String record) {
		this.record = record;
	}

	/**
	 * @return the controlPoint
	 */
	public String getControlPoint() {
		return controlPoint;
	}

	/**
	 * @param controlPoint the controlPoint to set
	 */
	public void setControlPoint(String controlPoint) {
		this.controlPoint = controlPoint;
	}
	

}
