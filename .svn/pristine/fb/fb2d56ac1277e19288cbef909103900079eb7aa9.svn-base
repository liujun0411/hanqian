/*-----菜单切换---------*/
$(function(){
		$('ul.MenuBarHorizontal li').hover(
			function(){
				$(this).find('ul').show();
				$(this).addClass('active');
				$(this).find('span').addClass('active');
			},
			function(){
				$(this).find('ul').hide();
				$(this).removeClass('active');
				$(this).find('span').removeClass('active');
			}
		);
	});